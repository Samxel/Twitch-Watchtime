{
  "action": {
     "default_title": "Twitch Watchtime"
  },
  "content_scripts": [
     {
        "js": [ "content.js"],
        "matches": [ "*://www.twitch.tv/*" ]
     }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "description": "Displays how long you've been watching a Twitch stream.",
  "manifest_version": 3,
  "name": "Twitch Watchtime",
  "permissions": ["scripting", "activeTab"],
  "version": "1.0"
}
